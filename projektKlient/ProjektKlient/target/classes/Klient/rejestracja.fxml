<VBox alignment="CENTER" fillWidth="false" maxHeight="350.0" maxWidth="300.0" spacing="20.0" style="-fx-background-color:green;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Klient.Rejestracja">
