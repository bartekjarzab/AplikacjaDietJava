      <TextField fx:id="zapotrzebowanie" alignment="CENTER" prefHeight="25.0" prefWidth="170.0" promptText="zapotrzebowanie kaloryczne" />
      <PasswordField fx:id="haslo" alignment="CENTER" prefHeight="25.0" prefWidth="170.0" promptText="HasÅ‚o" />
      <HBox alignment="CENTER" prefHeight="15.0" prefWidth="200.0" style="-fx-spacing: 40;">
         <children>
              <Button fx:id="zarejestruj" onAction="#rejestruj" text="Zarejestruj" />
            <Button fx:id="anuluj" alignment="CENTER" mnemonicParsing="false" onAction="#anuluj" prefHeight="18.0" prefWidth="52.0" text="Anuluj" />
